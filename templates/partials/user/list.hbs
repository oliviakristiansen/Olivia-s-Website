<h2>Users</h2>

<a class="btn btn-primary"href="/update/create">Create User</a>


{{#each data.usersList}}
    <div>
        <div>
            <strong>Name </strong> <a href="/update/{{id}}">{{username}}</a>
        </div>
        <div>
            <strong>E-Mail</strong> {{email}}
        </div>
        <div>
            <strong>Date Created</strong> {{createdOn}}
        </div>
        <div>
            <strong>Last Updated</strong> {{updatedOn}}
        </div>
        <div>
            <strong>Type</strong> {{type}}
        </div>
        <div>
            <strong>Status</strong> {{status}}
        </div>
    </div>
{{/each}}
